{
  "name": "riotjs-project-template",
  "version": "0.1.0",
  "main": "src/index.js",
  "repository": "https://github.com/chaucerbao/riotjs-project-template",
  "author": "Alvin Chan <no@email.com>",
  "license": "MIT",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server",
    "build": "NODE_ENV=production node_modules/.bin/webpack -p",
    "pretty": "node_modules/.bin/prettier --single-quote --write webpack.config.js 'src/**/*.js'",
    "test": "ava"
  },
  "ava": {
    "require": [
      "babel-register",
      "regenerator-runtime/runtime"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "syntax-dynamic-import"
    ]
  },
  "dependencies": {
    "riot": "^3.3.1",
    "riot-route": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.23.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "html-webpack-plugin": "^2.28.0",
    "nock": "^9.0.6",
    "node-fetch": "^1.6.3",
    "node-sass": "^4.5.0",
    "prettier": "^0.18.0",
    "regenerator": "^0.9.7",
    "riotjs-loader": "^4.0.0",
    "sass-loader": "^6.0.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
